@ARTICLE{917504,
  author = {Xylomenos, G. and Polyzos, G.C. and Mahonen, P. and Saaranen, M.},
  title = {TCP performance issues over wireless links},
  journal = {Communications Magazine, IEEE},
  year = {2001},
  volume = {39},
  pages = {52-58},
  number = {4},
  abstract = {This article discusses the problems arising when the TCP/IP protocol
	suite is used to provide Internet connectivity over existing and
	emerging wireless links. Due to the strong drive toward wireless
	Internet access through mobile terminals, these problems must be
	carefully studied in order to build improved systems. We review wireless
	link characteristics using wireless LANs and cellular communications
	systems as examples. We then outline the performance problems of
	the TCP/IP protocol suite when employed over those links, such as
	degraded TCP performance due to mistaking wireless errors for congestion.
	We present various proposals for solving these problems and examine
	their benefits and limitations. Finally, we consider the future evolution
	of wireless systems and the challenges that emerging systems will
	impose on the Internet protocol suite},
  bdsk-url-1 = {http://dx.doi.org/10.1109/35.917504},
